{
  "$schema": "./node_modules/nx/schemas/workspace-schema.json",
  "version": 2,
  "projects": {
    "testing": {
      "root": ".",
      "implicitDependencies": [
        "utils-eslint-config",
        "vibrant-components",
        "vibrant-core",
        "vibrant-example-app",
        "vibrant-icons",
        "vibrant-motion",
        "vibrant-theme",
        "vibrant-utils",
        "workspace-plugin"
      ],
      "targets": {
        "lint": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "exit 0"
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "exit 0"
          }
        },
        "typecheck": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "exit 0"
          }
        },
        "collect-coverage": {
          "executor": "@class101/workspace-plugin:merge-coverage",
          "dependsOn": [
            {
              "projects": "dependencies",
              "target": "test",
              "params": "forward"
            }
          ]
        }
      }
    },
    "utils-eslint-config": "packages/utils-eslint-config",
    "vibrant-components": "packages/vibrant-components",
    "vibrant-core": "packages/vibrant-core",
    "vibrant-example-app": "packages/vibrant-example-app",
    "vibrant-icons": "packages/vibrant-icons",
    "vibrant-motion": "packages/vibrant-motion",
    "vibrant-theme": "packages/vibrant-theme",
    "vibrant-utils": "packages/vibrant-utils",
    "vibrant-website": "packages/vibrant-website",
    "workspace-plugin": "packages/workspace-plugin"
  }
}
