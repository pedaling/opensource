{
  "$schema": "./node_modules/nx/schemas/workspace-schema.json",
  "version": 2,
  "projects": {
    "testing": {
      "root": ".",
      "implicitDependencies": [
        "utils-eslint-config",
        "vibrant-components",
        "vibrant-components-web",
        "vibrant-core",
        "vibrant-forms",
        "vibrant-example-app",
        "vibrant-icons",
        "vibrant-motion",
        "vibrant-theme",
        "vibrant-utils",
        "workspace-plugin"
      ],
      "targets": {
        "lint": {
          "executor": "nx:run-commands",
          "options": {
            "command": "exit 0"
          }
        },
        "test": {
          "executor": "nx:run-commands",
          "options": {
            "command": "exit 0"
          }
        },
        "typecheck": {
          "executor": "nx:run-commands",
          "options": {
            "command": "exit 0"
          }
        },
        "collect-coverage": {
          "executor": "@class101/workspace-plugin:merge-coverage",
          "dependsOn": [
            {
              "projects": "dependencies",
              "target": "test",
              "params": "forward"
            }
          ]
        }
      }
    },
    "utils-eslint-config": "packages/utils-eslint-config",
    "vibrant-benchmark-app": {
      "root": "packages/vibrant-benchmark-app",
      "sourceRoot": "packages/vibrant-benchmark-app/src",
      "projectType": "application",
      "targets": {
        "start": {
          "executor": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "serve": {
          "executor": "nx:run-commands",
          "options": {
            "command": "nx start vibrant-benchmark-app"
          }
        },
        "run-ios": {
          "executor": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "executor": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "src/main.tsx",
            "platform": "ios",
            "bundleOutput": "dist/packages/vibrant-benchmark-app/ios/main.jsbundle"
          }
        },
        "run-android": {
          "executor": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "executor": "@nrwl/react-native:build-android",
          "outputs": [
            "{projectRoot}/android/app/build/outputs/bundle",
            "{projectRoot}/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "executor": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "src/main.tsx",
            "platform": "android",
            "bundleOutput": "dist/packages/vibrant-benchmark-app/android/main.jsbundle"
          }
        },
        "sync-deps": {
          "executor": "@nrwl/react-native:sync-deps",
          "options": {}
        },
        "ensure-symlink": {
          "executor": "@nrwl/react-native:ensure-symlink",
          "options": {}
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "packages/vibrant-benchmark-app/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
          "options": {
            "jestConfig": "packages/vibrant-benchmark-app/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "vibrant-benchmark-app-e2e": {
      "root": "packages/vibrant-benchmark-app-e2e",
      "sourceRoot": "packages/vibrant-benchmark-app-e2e/src",
      "projectType": "application",
      "targets": {
        "build-ios": {
          "executor": "@nrwl/detox:build",
          "options": {
            "detoxConfiguration": "ios.sim.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "ios.sim.release"
            }
          }
        },
        "test-ios": {
          "executor": "@nrwl/detox:test",
          "options": {
            "detoxConfiguration": "ios.sim.debug",
            "buildTarget": "vibrant-benchmark-app-e2e:build-ios"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "ios.sim.release",
              "buildTarget": "vibrant-benchmark-app-e2e:build-ios:production"
            }
          }
        },
        "build-android": {
          "executor": "@nrwl/detox:build",
          "options": {
            "detoxConfiguration": "android.emu.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "android.emu.release"
            }
          }
        },
        "test-android": {
          "executor": "@nrwl/detox:test",
          "options": {
            "detoxConfiguration": "android.emu.debug",
            "buildTarget": "vibrant-benchmark-app-e2e:build-ios"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "android.emu.release",
              "buildTarget": "vibrant-benchmark-app-e2e:build-ios:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "packages/vibrant-benchmark-app-e2e/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["vibrant-benchmark-app"]
    },
    "vibrant-components": "packages/vibrant-components",
    "vibrant-components-web": "packages/vibrant-components-web",
    "vibrant-core": "packages/vibrant-core",
    "vibrant-example-app": "packages/vibrant-example-app",
    "vibrant-forms": "packages/vibrant-forms",
    "vibrant-icons": "packages/vibrant-icons",
    "vibrant-motion": "packages/vibrant-motion",
    "vibrant-theme": "packages/vibrant-theme",
    "vibrant-utils": "packages/vibrant-utils",
    "vibrant-website": "packages/vibrant-website",
    "workspace-plugin": "packages/workspace-plugin"
  }
}
